// Advanced Role-Based Access Control (RBAC) System

model Role {
  id              String   @id @default(cuid())
  agencyId        String
  agency          Agency   @relation(fields: [agencyId], references: [id], onDelete: Cascade)
  
  // Role Details
  name            String
  description     String?
  slug            String
  level           Int      @default(0) // Higher number = higher privilege
  isSystemRole    Boolean  @default(false)
  isActive        Boolean  @default(true)
  
  // Hierarchy
  parentId        String?
  parent          Role?    @relation("role_hierarchy", fields: [parentId], references: [id])
  children        Role[]   @relation("role_hierarchy")
  
  // Metadata
  color           String?
  icon            String?
  customFields    String?  // JSON additional configuration
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  // Relations
  users           UserRoleAssignment[]
  permissions     RolePermission[]
  
  @@unique([agencyId, slug])
  @@map("roles")
}

model Permission {
  id              String   @id @default(cuid())
  
  // Permission Details
  name            String
  slug            String   @unique
  description     String?
  category        PermissionCategory
  resource        String   // e.g., "students", "invoices", "commissions"
  action          String   // e.g., "create", "read", "update", "delete"
  
  // Configuration
  isSystemPermission Boolean @default(false)
  isActive        Boolean  @default(true)
  
  // Dependencies
  dependencies    String?  // JSON array of permission slugs this depends on
  
  // Metadata
  metadata        String?  // JSON additional configuration
  sortOrder       Int      @default(0)
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  // Relations
  rolePermissions RolePermission[]
  userPermissions UserPermission[]
  featureAccess   FeaturePermission[]
  
  @@map("permissions")
}

model RolePermission {
  id              String   @id @default(cuid())
  roleId          String
  role            Role     @relation(fields: [roleId], references: [id], onDelete: Cascade)
  permissionId    String
  permission      Permission @relation(fields: [permissionId], references: [id], onDelete: Cascade)
  
  // Access Control
  accessLevel     RBACAccessLevel @default(FULL)
  conditions      String?  // JSON conditions for this permission
  restrictions    String?  // JSON restrictions (time, IP, etc.)
  
  // Status
  isGranted       Boolean  @default(true)
  expiresAt       DateTime?
  
  // Metadata
  grantedBy       String?  // User ID who granted this permission
  grantedAt       DateTime @default(now())
  notes           String?
  
  @@unique([roleId, permissionId])
  @@map("role_permissions")
}

