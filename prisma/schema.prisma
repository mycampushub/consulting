// Advanced Role-Based Access Control (RBAC) Models

model Permission {
  id              String   @id @default(cuid())
  name            String   @unique
  description     String?
  slug            String   @unique
  category        PermissionCategory
  resource        String
  action          PermissionAction
  
  // Hierarchy
  parentId        String?
  parent          Permission? @relation("permission_children", fields: [parentId], references: [id])
  children        Permission[] @relation("permission_children")
  
  // Configuration
  conditions      String?  // JSON conditions for permission
  isSystem        Boolean  @default(false)
  isActive        Boolean  @default(true)
  
  // Metadata
  metadata        String?  // JSON additional data
  sortOrder       Int      @default(0)
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  // Relations
  rolePermissions RolePermission[]
  userPermissions UserPermission[]
  
  @@map("permissions")
}

model Role {
  id              String   @id @default(cuid())
  name            String
  description     String?
  slug            String   @unique
  category        RoleCategory
  level           RoleLevel
  
  // Permissions
  permissions     RolePermission[]
  
  // Hierarchy
  parentId        String?
  parent          Role? @relation("role_children", fields: [parentId], references: [id])
  children        Role[] @relation("role_children")
  
  // Configuration
  isSystem        Boolean  @default(false)
  isActive        Boolean  @default(true)
  isCustom        Boolean  @default(false)
  
  // Scope
  scope           RoleScope @default(GLOBAL)
  agencyId        String?
  agency          Agency?   @relation(fields: [agencyId], references: [id], onDelete: Cascade)
  branchId        String?
  branch          Branch?   @relation(fields: [branchId], references: [id])
  
  // Metadata
  metadata        String?  // JSON role configuration
  sortOrder       Int      @default(0)
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  // Relations
  userRoles       UserRoleAssignment[]
  
  @@unique([slug, agencyId, branchId])
  @@map("roles")
}

model UserPermission {
  id              String   @id @default(cuid())
  userId          String
  user            User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  permissionId    String
  permission      Permission @relation(fields: [permissionId], references: [id], onDelete: Cascade)
  
  // Access Control
  accessLevel     AccessLevel @default(FULL)
  restrictions    String?  // JSON restrictions
  conditions      String?  // JSON conditions
  
  // Scope
  scope           PermissionScope @default(GLOBAL)
  agencyId        String?
  agency          Agency?   @relation(fields: [agencyId], references: [id], onDelete: Cascade)
  branchId        String?
  branch          Branch?   @relation(fields: [branchId], references: [id])
  
  // Time-based
  expiresAt       DateTime?
  
  // Status
  isActive        Boolean  @default(true)
  
  // Metadata
  metadata        String?  // JSON additional data
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  @@unique([userId, permissionId, agencyId, branchId])
  @@map("user_permissions")
}

model RolePermission {
  id              String   @id @default(cuid())
  roleId          String
  role            Role     @relation(fields: [roleId], references: [id], onDelete: Cascade)
  permissionId    String
  permission      Permission @relation(fields: [permissionId], references: [id], onDelete: Cascade)
  
  // Access Control
  accessLevel     AccessLevel @default(FULL)
