model Branch {
  id              String   @id @default(cuid())
  agencyId        String
  agency          Agency   @relation(fields: [agencyId], references: [id], onDelete: Cascade)
  
  // Branch Details
  name            String
  code            String   
  type            BranchType @default(BRANCH)
  status          BranchStatus @default(PENDING)
  
  @@unique([agencyId, code]) // Unique branch code within agency
  
  // Contact Information
  email           String?
  phone           String?
  address         String?
  city            String?
  state           String?
  country         String?
  postalCode      String?
  
  // Location
  latitude        Float?
  longitude       Float?
  
  // Management
  managerId       String? // User ID of branch manager
  manager         User?    @relation("managedBranches", fields: [managerId], references: [id])
  businessHours   String?  // JSON configuration of business hours
  
  // Capacity & Limits
  maxStudents     Int?     // Maximum number of students this branch can handle
  maxStaff        Int?     // Maximum number of staff members
  
  // Metadata
  description     String?
  features        String?  // JSON array of enabled features
  settings        String?  // JSON configuration settings
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  // Relations
  users           User[] @relation("user_branch")
  students        Student[]
  applications    Application[]
  documents       Document[]
  featureAccess   FeatureAccess[]
  userRoleAssignments UserRoleAssignment[]
  roles           Role[]
  userPermissions UserPermission[]
  rolePermissions RolePermission[]
  userRestrictions UserRestriction[]
  roleRestrictions RoleRestriction[]
  resourceRestrictions ResourceRestriction[]
  
  @@map("branches")
}

// Subscription Management System

model SubscriptionPlan {
  id              String   @id @default(cuid())
  name            String
  description     String?
  slug            String   @unique
  status          PlanStatus @default(ACTIVE)
  
  // Pricing
  basePrice       Float    @default(0)
  currency        String   @default("USD")
  billingCycle    BillingCycle @default(MONTHLY)
  trialDays       Int      @default(0)
  
  // Capacity Limits
  maxUsers        Int      @default(1)
  maxStudents     Int      @default(0)
  maxStorage      Int      @default(1000) // in MB
  maxBranches     Int      @default(1)
  
  // Features
  features        PlanFeature[]
  
  // Metadata
  metadata        String?  // JSON additional configuration
  sortOrder       Int      @default(0)
  isPopular       Boolean  @default(false)
  isVisible       Boolean  @default(true)
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  // Relations
  subscriptions   AgencySubscription[]
  
  @@map("subscription_plans")
}

model SubscriptionFeature {
  id              String   @id @default(cuid())
